<Page x:Class="LabWork27.Pages.MainPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:LabWork27.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Главная">

    <StackPanel Margin="10" HorizontalAlignment="Center" VerticalAlignment="Center" Width="125" >
        <Button x:Name="AuthorizationPageButton" Margin="10"  Content="Авторизация" Height="30" Click="AuthorizationPageButton_Click"/>
        <Button x:Name="RegistrationPageButton" Margin="10" Content="Регистрация" Height="30" Click="RegistrationPageButton_Click"/>
        <Button x:Name="SettingsPageButton" Margin="10" Content="Настройка" Height="30" Click="SettingsPageButton_Click"/>
    </StackPanel>
</Page>
